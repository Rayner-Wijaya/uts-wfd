composer install // install dependency composer
npm install // pacakge manager buat laravel
--
php artisan serve // run biar website jalan 
npm run dev // run biar website jalan
--
php artisan make:model entity -mfs // membuat model enitity (entity diganti jadi yg lain klo mau buat yg lain). mfs adalah migration,factory,seeder. 
php artisan make:controller EntityController --resource // membuat sebuah controller resource (sudah ada function CRUD)
php artisan make:controller EntityController // membuat controller biasa
php artisan migrate // membuat tabel di database
php artisan migrate:fresh --seed // restart semua di database dan membuat tabel ulang dengan seeder yang sudah ditentukan
php artisan key:generate // untuk generate key pada application (dalam file .env)